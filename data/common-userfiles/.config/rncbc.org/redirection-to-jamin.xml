<!DOCTYPE patchbay>
<patchbay version="0.3.12" name="redirection-to-jamin">
 <output-sockets>
  <socket exclusive="off" client="jamin" type="jack-audio" name="Jamin output">
   <plug>a\.master\.out_L|out_L</plug>
   <plug>a\.master\.out_R|out_R</plug>
  </socket>
  <socket exclusive="off" client="MPlayer \[[0-9]+\]|mpv(|-.[0-9])" type="jack-audio" name="mplayer &amp; mpv">
   <plug>out_0</plug>
   <plug>out_1</plug>
  </socket>
  <socket exclusive="off" client="vlc_[0-9]+" type="jack-audio" name="VLC">
   <plug>out_1</plug>
   <plug>out_2</plug>
  </socket>
  <socket exclusive="off" client="TiMidity(|-.[0-9])" type="jack-audio" name="TiMidity">
   <plug>port_1</plug>
   <plug>port_2</plug>
  </socket>
  <socket exclusive="off" client="fluidsynth(|-.[0-9])|qsynth" type="jack-audio" name="fluidsynth">
   <plug>l_.*|left</plug>
   <plug>r_.*|right</plug>
  </socket>
  <socket exclusive="off" client="alsa-jack\.jackP\.[0-9]+\.[0-9]" type="jack-audio" name="ALSA">
   <plug>out_000</plug>
   <plug>out_001</plug>
  </socket>
  <socket exclusive="off" client="alsa_in" type="jack-audio" name="ALSA-aloop">
   <plug>capture_1</plug>
   <plug>capture_2</plug>
  </socket>
  <socket exclusive="off" client=".*" type="jack-audio" name="FIrefox (cubeb)">
   <plug>AudioStream_.*_0</plug>
   <plug>AudioStream_.*_1</plug>
  </socket>
  <socket exclusive="off" client="PulseAudio JACK Sink" type="jack-audio" name="PA stereo output">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
  <socket exclusive="off" client=".*" type="jack-audio" name="all GStreamer players">
   <plug>out_.*audiosink[\-0-9]+_1</plug>
   <plug>out_.*audiosink[\-0-9]+_2</plug>
  </socket>
  <socket exclusive="off" client="flash(|-.[0-9])|freshwrapper(|-.[0-9])" type="jack-audio" name="Adobe Flash (NPAPI &amp; PPAPI)">
   <plug>out1|output1</plug>
   <plug>out2|output2</plug>
  </socket>
 </output-sockets>
 <input-sockets>
  <socket exclusive="off" client="SimpleScreenRecorder" type="jack-audio" name="SimpleScreenRecorder">
   <plug>in_1</plug>
   <plug>in_2</plug>
  </socket>
  <socket exclusive="on" client="system" type="jack-audio" name="main output">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket exclusive="off" client="jamin" type="jack-audio" name="jamin volume boost">
   <plug>in_L</plug>
   <plug>in_R</plug>
  </socket>
  <socket exclusive="off" client="alsa_out" type="jack-audio" forward="PulseAudio Source" name="ALSA-aloop">
   <plug>playback_1</plug>
   <plug>playback_2</plug>
  </socket>
  <socket exclusive="off" client="PulseAudio JACK Source" type="jack-audio" name="PulseAudio Source">
   <plug>front-left</plug>
   <plug>front-right</plug>
  </socket>
 </input-sockets>
 <slots/>
 <cables>
  <cable output="Jamin output" input="main output" type="jack-audio"/>
  <cable output="mplayer &amp; mpv" input="jamin volume boost" type="jack-audio"/>
  <cable output="VLC" input="jamin volume boost" type="jack-audio"/>
  <cable output="TiMidity" input="jamin volume boost" type="jack-audio"/>
  <cable output="fluidsynth" input="jamin volume boost" type="jack-audio"/>
  <cable output="ALSA" input="jamin volume boost" type="jack-audio"/>
  <cable output="ALSA-aloop" input="jamin volume boost" type="jack-audio"/>
  <cable output="FIrefox (cubeb)" input="jamin volume boost" type="jack-audio"/>
  <cable output="PA stereo output" input="jamin volume boost" type="jack-audio"/>
  <cable output="all GStreamer players" input="jamin volume boost" type="jack-audio"/>
  <cable output="Adobe Flash (NPAPI &amp; PPAPI)" input="jamin volume boost" type="jack-audio"/>
 </cables>
</patchbay>

